# Google Gemini API
GOOGLE_API_KEY=your_gemini_api_key_here

# Tavily Search API
TAVILY_API_KEY=your_tavily_api_key_here

# Genel Ayarlar
LOG_LEVEL=INFO
MAX_CONCURRENT_SCRAPES=5
SCRAPE_TIMEOUT_SECONDS=30

# Model Ayarları
